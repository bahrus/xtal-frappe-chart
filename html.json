{
    "version": 1,
    "tags": [{
        "name": "xtal-frappe-chart",
        "description": "Web component wrapper around the cool Frappe chart (https://frappe.io/charts) library.",
        "selfResolvingModulePath": "https://unpkg.com/xtal-frappe-chart@0.0.33/xtal-frappe-chart.js?module",
        "testCaseNames": ["test1"],
        "attributes": [
            {
                "name": "data",
                "description": "Data to chart."
            }
        ],
        "properties":[
            {
                "name": "data",
                "description": "Data to chart.",
                "testValues": {
                    "test1": {
                        "title": "My Awesome Chart",
                        "data": {
                          "labels": ["12am-3am", "3am-6am", "6am-9am", "9am-12pm",
                            "12pm-3pm", "3pm-6pm", "6pm-9pm", "9pm-12am"],
                      
                          "datasets": [
                            {
                              "name": "Some Data", "color": "light-blue",
                              "values": [25, 40, 30, 35, 8, 52, 17, -4]
                            },
                            {
                              "name": "Another Set", "color": "violet",
                              "values": [25, 50, -10, 15, 18, 32, 27, 14]
                            },
                            {
                              "name": "Yet Another", "color": "blue",
                              "values": [15, 20, -3, -15, 58, 12, -17, 37]
                            }
                          ]
                        },
                        "type": "bar", 
                        "height": 250,
                        "isNavigable": true
                      }
                }
            },
            {
              "name": "selectedElement",
              "description": "Data corresponding to selected chart element.",
              "testValues": {
                "test1": [8,18,58]
              }
            }            
        ],
        "events": [
          {
            "name": "selected-element-changed",
            "testExpectedValues":{
              "test1":{
                "value": [8,18,58] 
              }
            }
          }
        ]
    }]
}